{
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.includeLanguages": {
    "plaintext": "html",
    "postcss": "css"
  },
  "tailwindCSS.experimental.classRegex": [
    ["class(?:Name)?=\"([^\"]+)\"", "class:\\s*`([^`]+)`"]
  ]
}
